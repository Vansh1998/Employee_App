<p id="notice"><%= notice %></p>

<h1>Employee Details</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Emp</th>
      <th>Emp designation</th>
      <th>Emp mail</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_details.each do |employee_detail| %>
      <tr>
        <td><%= employee_detail.first_name %></td>
        <td><%= employee_detail.last_name %></td>
        <td><%= employee_detail.emp_id %></td>
        <td><%= employee_detail.emp_Designation %></td>
        <td><%= employee_detail.emp_mail_id %></td>
        <td><%= link_to 'Show', employee_detail %></td>
        <td><%= link_to 'Edit', edit_employee_detail_path(employee_detail) %></td>
        <td><%= link_to 'Destroy', employee_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee Detail', new_employee_detail_path %>
